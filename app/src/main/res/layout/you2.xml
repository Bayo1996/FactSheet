<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/colorAccentt"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:layout_height="match_parent">
    <android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="fill_vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:scrollbars="vertical">
        <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:orientation="vertical">
                <android.support.v7.widget.CardView
                    android:id="@+id/anatomy"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="20dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="ANATOMY and PHYSIOLOGY"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp" />
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="Describe the male and female reproductive health system
"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">

                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_rectangle"
                                    android:gravity="center"
                                    android:padding="5dp"
                                    android:textColor="#fff"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/carddd"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="@string/sex_and_sexuality"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp" />
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="Sexually healthy, Health sex life, Sex and Partner, Queer, Asexuality, Reproductive Health"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">

                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_rectangle"
                                    android:gravity="center"
                                    android:padding="5dp"
                                    android:textColor="#fff"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/TB"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/consent"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp" />
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="What is sexual consent"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">

                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_rectangle"
                                    android:gravity="center"
                                    android:padding="5dp"
                                    android:textColor="#fff"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/cardt"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="SEXUAL GENDER BASED VIOLENCE"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp" />
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="What is sexual assaults, abuse and rape"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">

                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_rectangle"
                                    android:gravity="center"
                                    android:padding="5dp"
                                    android:textColor="#fff"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5"
                            android:baselineAligned="false">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/family_planning"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15sp"/>
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="What is fp, Contraceptives, How to get fp"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">
                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_circle"
                                    android:gravity="center"
                                    android:textColor="#fff"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/men"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="MENSTRUATION and CONCEPTION"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp" />
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:text="Premenstrual Syndrome, Physical and Emotional Characteristics"
                                    android:textAlignment="center"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.2"
                                android:gravity="center">

                                <TextView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@drawable/orange_rectangle"
                                    android:gravity="center"
                                    android:padding="5dp"
                                    android:textColor="#fff"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/child"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginTop="15dp"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/background">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_margin="5dp"
                            android:orientation="horizontal"
                            android:weightSum="5">


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3.8"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                android:paddingLeft="10dp">

                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/teenage_pregnancy"
                                    android:ellipsize="marquee"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorAccentsS"
                                    android:textSize="15dp"/>
                                <com.HIVFactsheet.ScrollingTextview
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:ellipsize="marquee"
                                    android:background="@drawable/gray_rectangle"
                                    android:gravity="center"
                                    android:textColor="@color/colorAccentts"
                                    android:textSize="10sp"
                                    android:focusable="true"
                                    android:focusableInTouchMode="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:singleLine="true"
