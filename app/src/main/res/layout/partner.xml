<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/colorAccentt"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:layout_height="match_parent">
    <android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="fill_vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:scrollbars="vertical">
        <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view1"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="30dp"
                card_view:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:orientation="horizontal"
                    android:weightSum="4">
                    <View
                        android:layout_width="5dp"
                        android:layout_height="40dp"
                        android:background="@color/colorBackroundWhite">
                    </View>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.8"
                        android:gravity="center_vertical"
                        android:orientation="vertical">
                        <com.HIVFactsheet.ScrollingTextview
                            android:id="@+id/text1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="20dp"
                            android:layout_marginStart="20dp"
                            android:ellipsize="marquee"
                            android:focusable="true"
                            android:focusableInTouchMode="false"
                            android:marqueeRepeatLimit="marquee_forever"
                            android:singleLine="true"
                            android:gravity="center"
                            android:text="Peer Education"
                            android:textAlignment="center"
                            android:textColor="@color/colorAccentsS"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.2"
                        android:gravity="center">
                          <View
                            android:layout_width="5dp"
                            android:layout_height="40dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <View
                            android:layout_width="40dp"
                            android:layout_height="5dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                    </LinearLayout>
                </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/card"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="25dp"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:weightSum="4">
                        <View
                            android:layout_width="5dp"
                            android:layout_height="40dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.8"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <com.HIVFactsheet.ScrollingTextview
                                android:id="@+id/textf"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_marginStart="20dp"
                                android:ellipsize="marquee"
                                android:focusable="true"
                                android:focusableInTouchMode="false"
                                android:marqueeRepeatLimit="marquee_forever"
                                android:singleLine="true"
                                android:gravity="center"
                                android:text="MTRH Rafiki Center"
                                android:textAlignment="center"
                                android:textColor="@color/colorAccentsS" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center">
                            <View
                                android:layout_width="40dp"
                                android:layout_height="5dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                            <View
                                android:layout_width="5dp"
                                android:layout_height="40dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/card_view"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="25dp"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:weightSum="4">
                        <View
                            android:layout_width="5dp"
                            android:layout_height="60dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.8"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <com.HIVFactsheet.ScrollingTextview
                                android:id="@+id/textf4"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_marginStart="20dp"
                                android:ellipsize="marquee"
                                android:focusable="true"
                                android:focusableInTouchMode="false"
                                android:marqueeRepeatLimit="marquee_forever"
                                android:singleLine="true"
                                android:gravity="center"
                                android:text="Aunty Jane"
                                android:textAlignment="center"
                                android:textColor="@color/colorAccentsS" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center">
                            <View
                                android:layout_width="5dp"
                                android:layout_height="40dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                            <View
                                android:layout_width="40dp"
                                android:layout_height="5dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/card_view3"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="25dp"
                card_view:cardElevation="8dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:weightSum="4">
                        <View
                            android:layout_width="5dp"
                            android:layout_height="60dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.8"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <com.HIVFactsheet.ScrollingTextview
                                android:id="@+id/text3"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_marginStart="20dp"
                                android:ellipsize="marquee"
                                android:focusable="true"
                                android:focusableInTouchMode="false"
                                android:marqueeRepeatLimit="marquee_forever"
                                android:singleLine="true"
                                android:gravity="center"
                                android:text="IYAFP"
                                android:textAlignment="center"
                                android:textColor="@color/colorAccentsS" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center">
                            <View
                                android:layout_width="40dp"
                                android:layout_height="5dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                            <View
                                android:layout_width="5dp"
                                android:layout_height="40dp"
                                android:background="@color/colorBackroundWhite">
                            </View>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/card_view4"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="25dp"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:weightSum="4">
                        <View
                            android:layout_width="5dp"
                            android:layout_height="60dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.8"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <com.HIVFactsheet.ScrollingTextview
                                android:id="@+id/text6"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_marginStart="20dp"
                                android:ellipsize="marquee"
                                android:focusable="true"
                                android:focusableInTouchMode="false"
                                android:marqueeRepeatLimit="marquee_forever"
                                android:singleLine="true"
                                android:gravity="center"
                                android:text="One2One - 1190"
                                android:textAlignment="center"
                                android:textColor="@color/colorAccentsS"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center">
                            <com.mikhaellopez.circularimageview.CircularImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/profilde_images"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="30dp"
                                app:civ_border_color="@color/colorBackroundWhite"
                                app:civ_border_width="2dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/card_view5"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_marginTop="25dp"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:weightSum="4">
                        <View
                            android:layout_width="5dp"
                            android:layout_height="60dp"
                            android:background="@color/colorBackroundWhite">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.8"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <com.HIVFactsheet.ScrollingTextview
                                android:id="@+id/text4"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="20dp"
                                android:layout_marginStart="20dp"
                                android:ellipsize="marquee"
                                android:focusable="true"
                                android:focusableInTouchMode="false"
                                android:marqueeRepeatLimit="marquee_forever"
                                android:singleLine="true"
                                android:gravity="center"
                                android:text="Adolescent Hub"
                                android:textAlignment="center"
                                android:textColor="@color/colorAccentsS"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.2"
                            android:gravity="center">
                            <com.mikhaellopez.circularimageview.CircularImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/profile_images"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="30dp"
                                app:civ_border_color="@color/colorBackroundWhite"
                                app:civ_border_width="2dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        </ScrollView>
    </android.support.v4.widget.NestedScrollView>
</RelativeLayout>