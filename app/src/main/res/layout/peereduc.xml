<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@mipmap/bg"
    android:layout_height="match_parent">
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:layout_scrollFlags="scroll|enterAlways"
            android:theme="@style/ThemeOverlay.AppTheme.ActionBar"
            app:popupTheme="@style/ThemeOverlay.AppTheme.PopupMenu">
            <com.HIVFactsheet.ScrollingTextview
                android:id = "@+id/title"
                android:textColor = "@color/colorAccentsss"
                android:textSize="20sp"
                android:layout_width = "wrap_content"
                android:marqueeRepeatLimit="1"
                android:ellipsize="marquee"
                android:singleLine="true"
                android:textStyle="bold"
                android:layout_height = "wrap_content" />
        </android.support.v7.widget.Toolbar>
    </android.support.design.widget.AppBarLayout>

<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="fill_vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:scrollbars="vertical">

<ScrollView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="vertical">
<android.support.v7.widget.LinearLayoutCompat
    android:id="@+id/id"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="vertical">
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button345"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="40dp"
        android:text="LifeSkills"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight"
        />
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button200"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Facilitation Skills"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight"
        />
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button9002"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Peer MentorShip"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight"
        />
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button201"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Peer Counselling"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight" />
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button203"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Helping Youth Deal with Peer Pressure"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight" />
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button204"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Substances and Drug Addiction"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight"/>
    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button206"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Mental Health Issues"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight" />

    <com.HIVFactsheet.ScrollingButton
        android:id="@+id/button9001"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_alignEnd="@+id/button15"
        android:ellipsize="marquee"
        android:textColor="@android:color/black"
        android:focusable="true"
        android:layout_marginBottom="50dp"
        android:focusableInTouchMode="false"
        android:marqueeRepeatLimit="marquee_forever"
        android:singleLine="true"
        android:layout_alignRight="@+id/button15"
        android:layout_marginTop="30dp"
        android:text="Referral and Linkages"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:backgroundTint="?android:attr/colorMultiSelectHighlight"
        />
</android.support.v7.widget.LinearLayoutCompat>
</ScrollView>
</android.support.v4.widget.NestedScrollView>
    </android.support.v7.widget.LinearLayoutCompat>
</RelativeLayout>